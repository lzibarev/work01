<?xml version="1.0" encoding="UTF-8"?>
<function xmlns="http://documentum.emc.com/2010/UI-Model"
	id="JSFunction-dtbConvertFloatToStr" version="1.0.0000.001">

	<name>dtbConvertFloatToStr</name>
	<description>Converts a float value to a string</description>
	<content src="content/dtb/functions/convert.js" type="text/javascript"/>

	<namespace>dtb.functions</namespace>
	<categoryName>All</categoryName>

	<returnType isRepeating="false">STRING</returnType>
	<params>
		<param name="float" required="true">
			<type isRepeating="false">FLOAT</type>
		</param>
		<param name="precision" required="false">
			<type isRepeating="false">INTEGER</type>
		</param>
	</params>
</function>